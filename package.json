{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker-compose:local": "docker-compose -f devops/local-dev/docker-compose.yml down && docker-compose -f devops/local-dev/docker-compose.yml up --build"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/config": "0.0.9",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.1.10",
    "@nestjs/testing": "^7.0.9",
    "@nestjs/typeorm": "^6.2.0",
    "@types/crypto-js": "^3.1.46",
    "@types/form-data": "^2.5.0",
    "@types/sanitize-html": "^1.23.3",
    "aws-sdk": "^2.601.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "config": "^3.2.4",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "jszip": "^3.3.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "ms": "^2.1.2",
    "mustache": "^3.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.17.1",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sanitize-html": "^1.27.0",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.22",
    "uuid": "^7.0.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/jszip": "^3.1.7",
    "@types/mocha": "^7.0.2",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.3.10",
    "@types/mustache": "^3.2.0",
    "@types/node": "^12.7.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@types/xml2js": "^0.4.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "jest": "^24.9.0",
    "mocha": "^7.1.2",
    "node-mocks-http": "^1.8.1",
    "prettier": "^1.18.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-loader": "^6.1.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}